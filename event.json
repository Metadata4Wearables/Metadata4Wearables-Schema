{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "Event",
  "description": "Event definition in the context of actigraphy",
  "type": "object",
  "properties": {
    "event_id": {
      "description": "The unique identifier for an event",
      "type": "integer",
      "minimum": 0,
      "unique": true
    },
    "event_name": {
      "description": "Name of the event",
      "type": "string"
    },
    "start_date_time": {
      "description": "Start date-time for an event",
      "type": "date-time"
    },
    "stop_date_time": {
      "description": "Stop date-time for an event",
      "type": "date-time"
    },
    "comment": {
      "description": "Comment about an event",
      "type": "string"
    }
  },
  "required": [ "event_id", "event_name", "start_date_time", "stop_date_time"],
  "missingValues": ["NaN"]
}
